    "@testing-library/react": "^9.1.4",
    "@types/cheerio": "^0.22.15",
    "@types/jest": "24.0.18",
    "@types/node": "12.7.5",
    "@types/react": "16.9.2",
    "@types/react-dom": "16.9.0",
    "@types/sortablejs": "file:local-types/sortablejs",
    "@types/styled-components": "^4.1.19",
    "cheerio": "^1.0.0-rc.3",
    "parcel-bundler": "^1.12.4",
    "react": "^16.9.0",
    "react-dom": "^16.9.0",
    "styled-components": "^4.3.2",
    "ts-node": "^8.5.4",
